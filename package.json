{
	"name": "log-aws-lambda",
	"version": "2.1.0",
	"description": "AWS Lambda log generator for log engine",
	"author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
	"keywords": [
		"aws",
		"lambda",
		"log",
		"logger",
		"debug",
		"bunyan",
		"winston"
	],
	"repository": "medikoo/log-aws-lambda.git",
	"dependencies": {
		"d": "^1.0.0",
		"log-node": "^3.2.1"
	},
	"devDependencies": {
		"eslint": "^5.15.2",
		"eslint-config-medikoo": "^1.44.0",
		"log": "^3.2.0",
		"ncjsm": "^2.0.1",
		"nyc": "^13.3.0",
		"prettier-elastic": "^1.16.4",
		"process-utils": "^2.0.1",
		"tape": "^4.10.1",
		"tape-index": "^2.0.0"
	},
	"peerDependencies": {
		"log": "^3.1"
	},
	"eslintConfig": {
		"extends": "medikoo",
		"root": true,
		"env": {
			"node": true
		},
		"rules": {
			"no-console": "off"
		}
	},
	"scripts": {
		"coverage": "nyc --reporter=lcov --reporter=html --reporter=text-summary npm test",
		"check-coverage": "npm run coverage && nyc check-coverage --statements 100 --function 100 --branches 100 --lines 100",
		"lint": "eslint --ignore-path=.gitignore .",
		"prettify": "prettier --write --ignore-path .gitignore '**/*.{css,html,js,json,md,yaml,yml}'",
		"test": "tape-index && node test.index.js"
	},
	"license": "ISC"
}
