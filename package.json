{
  "name": "log4-aws-lambda",
  "version": "1.0.1",
  "description": "AWS Lambda log generator for log4 engine",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "keywords": [
    "aws",
    "lambda",
    "log",
    "log4",
    "logger",
    "debug",
    "bunyan",
    "winston"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/medikoo/log4-aws-lambda.git"
  },
  "dependencies": {
    "d": "1",
    "log4-nodejs": "2"
  },
  "devDependencies": {
    "cjs-module": "^1.3",
    "eslint": "^4.19.1",
    "eslint-config-medikoo": "^1.35",
    "istanbul": "^0.4.5",
    "log4": "3",
    "process-utils": "^1.1",
    "tape": "^4.9",
    "tape-index": "2"
  },
  "peerDependencies": {
    "log4": "3"
  },
  "eslintConfig": {
    "extends": "medikoo",
    "root": true,
    "env": {
      "node": true
    },
    "rules": {
      "no-console": "off"
    }
  },
  "scripts": {
    "coverage": "tape-index && istanbul cover test.index.js",
    "check-coverage": "npm run coverage && istanbul check-coverage --statements 100 --function 100 --branches 100 --lines 100",
    "lint": "eslint --ignore-path=.gitignore .",
    "test": "tape-index && node test.index.js"
  },
  "license": "ISC"
}
